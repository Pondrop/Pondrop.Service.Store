{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosConfiguration": {
    "DatabaseName": "COSMOS_DB_NAME",
    "ApplicationName": "COSMOS_DB_APP_NAME",
    "ConnectionString": "COSMOS_DB_CONNECTION_STRING"
  },
  "ServiceBusConfiguration": {
    "ConnectionString": "SERVICE_BUS_CONNECTION_STRING",
    "QueueName": "SERVICE_BUS_QUEUE_NAME"
  },
  "DaprEventTopicConfiguration": {
    "RetailerUpdateConfiguration": {
      "AppId": "RETAILER_UPDATE_APP_ID",
      "MethodName": "RETAILER_UPDATE_METHOD_NAME",
      "EventTopic": "RETAILER_UPDATE_EVENT_TOPIC"
    },
    "StoreTypeUpdateConfiguration": {
      "AppId": "STORE_TYPE_UPDATE_APP_ID",
      "MethodName": "STORE_TYPE_UPDATE_METHOD_NAME",
      "EventTopic": "STORE_TYPE_UPDATE_EVENT_TOPIC"
    },
    "StoreUpdateConfiguration": {
      "AppId": "STORE_UPDATE_APP_ID",
      "MethodName": "STORE_UPDATE_METHOD_NAME",
      "EventTopic": "STORE_UPDATE_EVENT_TOPIC"
    }
  }
}
